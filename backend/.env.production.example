# Database (Supabase) - Use connection pooler for production
DATABASE_URL="postgres://postgres.YOUR_PROJECT_ID:YOUR_PASSWORD@aws-0-region.pooler.supabase.com:6543/postgres?pgbouncer=true&sslmode=require"

# JWT - Use a strong secret in production
JWT_SECRET="your-production-jwt-secret-min-32-chars"
JWT_EXPIRES_IN="7d"

# Server
PORT=3002
NODE_ENV=production

# Frontend URL (for CORS) - Update with your Vercel URL
FRONTEND_URL="https://your-app.vercel.app"
